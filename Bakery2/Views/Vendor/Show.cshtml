@using System.Collections.Generic;
@using Bakery2.Models;

<h1>Vendor Details</h1>

<ul>
  @foreach (Vendor vendor in Vendor.vendors)
  {
    <li><p>Vendor Name:</p><a href='/vendors/details'>@vendor.Name</a></li>
    <li><p>Vendor Description:</p><a href='/vendors/details'>@vendor.Description</a></li>
    <br>
  }
</ul>

<a href="/vendors/new">Add a new vendor.</a>